<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@+id/viewMoodHistoryFragment">

    <fragment
        android:id="@+id/viewMoodHistoryFragment"
        android:name="com.example.mooderation.ViewMoodHistoryFragment"
        android:label="view_mood_history_fragment"
        tools:layout="@layout/view_mood_history_fragment">
        <action
            android:id="@+id/action_viewMoodHistoryFragment_to_addMoodEventFragment"
            app:destination="@id/addMoodEventFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_enter_anim"
            app:popExitAnim="@anim/nav_default_exit_anim" />
        <action
            android:id="@+id/action_viewMoodHistoryFragment_to_editMoodEventFragment"
            app:destination="@id/editMoodEventFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_enter_anim"
            app:popExitAnim="@anim/nav_default_exit_anim" />
    </fragment>

    <fragment
        android:id="@+id/addMoodEventFragment"
        android:name="com.example.mooderation.AddMoodEventFragment"
        android:label="add_mood_event_fragment"
        tools:layout="@layout/add_mood_event_fragment" />

    <fragment
        android:id="@+id/editMoodEventFragment"
        android:name="com.example.mooderation.EditMoodEventFragment"
        android:label="edit_mood_event_fragment"
        tools:layout="@layout/edit_mood_event_fragment" />



</navigation>